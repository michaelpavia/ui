{
  "name": "riipen-ui",
  "description": "Riipen React component UI kit.",
  "author": {
    "name": "Riipen",
    "email": "admin@riipen.com",
    "url": "https://riipen.com"
  },
  "repository": "https://github.com/riipen/ui",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "npm": ">=6.12.0"
  },
  "scripts": {
    "ci:audit": "audit-ci --moderate",
    "ci:test:coverage": "NODE_ENV=test NODE_PATH=./ nyc mocha --opts ./test/unit/ci.mocha.opts",
    "format:check": "prettier --check \"**/*.{js,jsx}\"",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:js": "eslint ."
  },
  "dependencies": {
    "react": "16.10.2"
  },
  "devDependencies": {
    "eslint": "6.5.1",
    "lerna": "3.18.1",
    "prettier": "1.18.2",
    "stylelint": "11.1.1"
  }
}
